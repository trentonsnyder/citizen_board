<%= link_to "back", auth_organization_path(@organization.id), class: 'primary-button' %>
<%= link_to "New Ticket", new_auth_pool_ticket_path(@pool.id), class: 'primary-button' %>
<div class='lanes-container'>
  <% @lanes.each do |lane| %>
    <div class='lane'>
    <h4><%= lane.name %></h4>
      <% lane.tickets.each do |ticket| %>
        <p><%= ticket.public_label %></p>
        <p><%= ticket.nickname %></p>
      <% end %>
    </div>
  <% end %>
</div>
